        "@types/eslint": "^8.4.10",
        "@types/marked": "^4.0.7",
        "@types/node": "^18.11.11",
        "@types/react": "^18.0.26",
        "@types/react-dom": "^18.0.9",
        "@vitejs/plugin-react": "^2.2.0",
        "@vitest/coverage-c8": "^0.25.4",
        "autoprefixer": "^10.4.13",
        "binode": "^1.0.5",
        "c8": "^7.12.0",
        "cookie": "*",
